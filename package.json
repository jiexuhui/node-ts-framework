{
  "name": "hjdw",
  "version": "1.0.8",
  "private": true,
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "pkg": {
    "scripts": [
      "dist/**/*.js"
    ]
  },
  "scripts": {
    "dev": "set NODE_ENV=dev && npm run build && concurrently --kill-others \"tsc -w -p src\" \"nodemon dist/index.js -e ts\" ",
    "debug": "npm run build && concurrently --kill-others \"tsc -w -p src\" \"nodemon --inspect dist/index.js -e ts\"",
    "test:watch": "mocha -r ts-node/register -w ./spec/**/*.spec.ts --watch-extensions ts",
    "test": "mocha -r ts-node/register -w ./spec/**/*.spec.ts",
    "build": "tsc -p src",
    "start": "node dist/index.js",
    "build:docker": "docker build -t rjmacarthy/express-typescript-starter .",
    "docker:compose": "docker-compose up",
    "format": "prettier --write 'src/**/*ts'",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:test": "tslint -c tslint.json 'spec/**/*.ts'",
    "format:test": "prettier --write 'spec/**/*ts'",
    "db": "rm -rf entities & npx typeorm-model-generator -h 47.104.92.151 -d hjdw2 -p 3306 -u root -x lnmp.org#25812 -e mysql -o ./src/entity/api --noConfig true --ce pascal --cp camel"
  },
